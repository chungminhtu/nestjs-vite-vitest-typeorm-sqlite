{
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    },
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "never"
    }
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    },
    "editor.formatOnSave": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.formatOnSave": false
  },
  "editor.smoothScrolling": true,
  "editor.smartSelect.selectSubwords": true,
  "editor.wordWrap": "on",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.guides.indentation": false,
  "html.format.wrapLineLength": 0,
  "files.defaultLanguage": "typescript",
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    // "**/*dist*": true,
    "**/*coverage*": true,
    "**/*node_modules": true,
    "**/*package-lock.json*": true,
    "**/pnpm-lock.yaml": true,
    "**/*yarn.lock*": true,
    "**/build": true,
    "**/apps/backend/libs": true,
    "**/.angular": true,
    "**/.docusaurus": true
  },
  "editor.fontFamily": "'Google Sans Code','MonoLisa Trial','Cascadia Code','Cousine','Jetbrains Mono','Fira Code',Consolas, 'SF Mono', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": false,
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "hediet.vscode-drawio.theme": "atlas",
  "vsicons.presets.angular": true,
  "workbench.iconTheme": "vscode-icons",
  "window.zoomLevel": 0,
  // "editor.fontSize": 14,
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.folders.associations": {
    "context": "app",
    "resolvers": "controller",
    "types": "class",
    "entities": "class",
    "migrations": "import",
    "seeds": "helper",
    "factories": "lib",
    "dto": "Docs"
  },
  "material-icon-theme.files.associations": {
    "*.repository.ts": "Nest-gateway",
    "*.entity.ts": "Nest-pipe",
    "*.config.ts": "Settings",
    "*.dto.ts": "Document",
    "*.mail.ts": "Email",
    "main.ts": "Nodejs"
  },
  "vscode-vitest.packageManager": "pnpm",
  "vscode-vitest.executableOptions": {
    "watch": true,
    "debug": true,
    "run": true,
  },
  "testing.automaticallyOpenPeekView": "never",
  "javascript.suggest.autoImports": true,
  "javascript.preferences.importModuleSpecifier": "relative",
  "typescript.tsdk": "node_modules/typescript/lib",
  "angular.enable-strict-mode-prompt": false,
  "[javascript][typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    }
  },
  "markdown.styles": [
    "https://cdn.jsdelivr.net/gh/dhdhagar/vscode-md-preview-light/style.min.css"
  ]
}